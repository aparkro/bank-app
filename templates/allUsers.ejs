<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <title>All Users</title>
    </head>
    <body>
        <h1>All Users</h1>
        <ul>
            <% users.forEach(user => { %>
                <li>Name: <%= user.name %>, Email: <%= user.email %>, Balance: <%= user.balance %></li>
                <img src="https://robohash.org/<%= user.email %>?size=100x100" alt="Profile Picture" style="border-radius: 50%;">
            <% }) %>
        </ul>
    </body>
</html>
